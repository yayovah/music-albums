{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.csr.html",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ],
        "urls": [
          "https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/**/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2)"
        ]
      }
    }
  ],

  "dataGroups": [
    {
      "name": "albums-api",
      "urls": ["https://musicbrainz.org/ws/2/release?query=date:2010&fmt=json"],
      "cacheConfig": {
        "maxSize": 100,
        "maxAge": "100h",
        "timeout": "2s",
        "strategy": "freshness"
      }
    },
    {
      "name": "portades-api",
      "urls": ["https://coverartarchive.org/release/**"],
      "cacheConfig": {
        "maxSize": 100,
        "maxAge": "100h",
        "timeout": "2s",
        "strategy": "freshness"
      }
    }
  ]
}
